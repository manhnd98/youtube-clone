{
    "name": "api-feature-prisma",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/api/feature-prisma/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/api/feature-prisma",
                "tsConfig": "libs/api/feature-prisma/tsconfig.lib.json",
                "packageJson": "libs/api/feature-prisma/package.json",
                "main": "libs/api/feature-prisma/src/index.ts",
                "assets": ["libs/api/feature-prisma/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/api/feature-prisma/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/api/feature-prisma"],
            "options": {
                "jestConfig": "libs/api/feature-prisma/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["scope:api", "type:feature", "context:database"]
}
